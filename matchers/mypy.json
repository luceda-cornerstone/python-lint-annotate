{
  "problemMatcher": [
    {
      "owner": "mypy",
      "severity": "error",
      "pattern": [
        {
          "regexp": "^([^:]*):(\\d+):(?:(\\d+):)? error: (.*?)(?:  \\[(\\S+)\\])?$",
          "file": 1,
          "column": 3,
          "line": 2,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}
